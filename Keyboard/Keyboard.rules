
(rules PCB Keyboard
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 731)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 3.6)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.4)
    )
  )
  (rule
    (width 375.0)
    (clear 200.2)
    (clear 187.5 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_600:400_um"
    (shape
      (circle F.Cu 600.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 600.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_600:400_um" "Via[0-1]_600:400_um" default
  )
  (via 
    "Via[0-1]_600:400_um-kicad_default" "Via[0-1]_600:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_600:400_um-Key_Signal" "Via[0-1]_600:400_um" "Key_Signal"
  )
  (via 
    "Via[0-1]_600:400_um-Leds" "Via[0-1]_600:400_um" Leds
  )
  (via 
    "Via[0-1]_600:400_um-Power" "Via[0-1]_600:400_um" Power
  )
  (via_rule
    default "Via[0-1]_600:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_600:400_um-kicad_default"
  )
  (via_rule
    "Key_Signal" "Via[0-1]_600:400_um-Key_Signal"
  )
  (via_rule
    Leds "Via[0-1]_600:400_um-Leds"
  )
  (via_rule
    Power "Via[0-1]_600:400_um-Power"
  )
  (class default
    PB7 PB6 PB5 PB4 PB3 PB2 PB1 PB0
    PA6 PA5 PA4 PA3 PA2 PA1 PA0 "Net-(R1-Pad1)"
    "Net-(D2-Pad2)" ~{RESET} LOCK "unconnected-(U1-Pad3)" "Net-(D1-Pad1)"
    (clearance_class default)
    (via_rule default)
    (rule
      (width 375.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 375.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "Key_Signal"
    (clearance_class "Key_Signal")
    (via_rule Key_Signal)
    (rule
      (width 750.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class Leds
    (clearance_class Leds)
    (via_rule Leds)
    (rule
      (width 500.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class Power
    VCC GND
    (clearance_class Power)
    (via_rule Power)
    (rule
      (width 1000.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)